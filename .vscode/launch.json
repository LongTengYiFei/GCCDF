    "version": "0.2.0",
    "configurations": [{
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "/home/cyf/githubDownload/GCCDF/build/Odess",
        "args": [
            "--task=batch",
            "--BatchFilePath=/home/cyf/githubDownload/GCCDF/build/batch_file_path.txt",
            "--RestorePath=/home/cyf/ssd0/restore/rFile",
            "--LCbasedGC=true",
            "--RewritingMethod=capping",
            "--CappingThreshold=20",
            "--enable_rewriting=false",
            "--GCInterval=20"
        ],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }]
}